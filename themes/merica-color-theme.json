{
	"name": "Merica",
	"type": "dark",
	"colors": {
		"editor.background": "#15161b",
		"editor.foreground": "#ffffff",
		"editor.selectionBackground": "#7259b1",
		"editor.inactiveSelectionBackground": "#3b3b3b",
		"editor.lineHighlightBackground": "#21222c",
		"editorCursor.foreground": "#ffffff",
		"editorWhitespace.foreground": "#313024"
	},
	"tokenColors": [
		{
			"settings": {
				"foreground": "#ffffffff",
				"background": "#15161bff"
			}
		},
		{
			"name": "String",
			"scope": "string",
			"settings": {
				"foreground": "#ddd9b8"
			}
		},
		{
			"name": "Square brace",
			"scope": "meta.brace.square, punctuation.definition.binding-pattern",
			"settings": {
				"foreground": "#B5BCDA"
			}
		},
		{
			"name": "Expression",
